<?xml version="1.0"?>
<package>
  <metadata>
    <id>Tiver.Fowl</id>
    <version>0.1.7</version>
    <authors>Artsiomi Silivonchyk</authors>
    <owners>Artsiomi Silivonchyk</owners>
    <license type="expression">MIT</license>
    <projectUrl>https://github.com/MrHant/tiver-fowl</projectUrl>
