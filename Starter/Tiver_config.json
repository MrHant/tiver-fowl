{
  "ApplicationConfiguration": {
    "Title": "System Under Test Title",
  },
  "BrowserConfiguration": {
    "BrowserType": "chrome",
    "DownloadBinary": true,
    "Headless": false,
    "Resolution": {
      "Width": 800,
      "Height": 600
    }
  },
  "Tiver.Fowl.Drivers": {
    "HttpTimeout": 120,
    "Drivers": [
      {
        "Name": "chrome",
        "DownloaderType": "ChromeDriverDownloader",
        "Version": "LATEST_RELEASE",
        "Platform": "win32"
      }
    ]
  },
  "Tiver.Fowl.Waiting": {
    "Timeout": 5000,
    "PollingInterval": 250,
    "IgnoredExceptionsTypeNames": [
      "OpenQA.Selenium.NoSuchElementException, WebDriver",
      "OpenQA.Selenium.StaleElementReferenceException, WebDriver"
    ]
  }
}
